
.Hybody {
	/* padding: 0 32upx; */
	height: 100%;
	font-size: 12px;
	background-color: #080808;
	color: #fff;
}
.title{
	color: rgba(255, 255, 255, 100);
	font-size: 22px;
	text-align: left;
	font-weight: 600;
	font-family: 方正工业黑-标准;
	margin-bottom: 10rpx;
	padding: 20rpx 32rpx;
}
.content{
	height: 1500rpx;
	line-height: 20px;
	border-radius: 45px 45px 0px 0px;
	text-align: center;
	background-color: rgb(45,46,41);
}
.box{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding:30rpx 50rpx;
}
.boxleft{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.img{
	border-radius: 50%;
}
.img image{
	width:121rpx;
	height: 121rpx;
	border-radius: 50%;
}
.text {
	margin-left: 10rpx;
}
.num{
	width: 44rpx;
	height: 44rpx;
	background-color:#F36D11;
	border-radius: 50%;
}

