
.box{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding:30rpx 50rpx;
}
.boxleft{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.boxmiddle{
	width: 600rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 121rpx;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
}
.boxright{
	width: 80rpx;
	height: 121rpx;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
}
.boxright .num{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-top: 10rpx;
}
.img{
	border-radius: 50%;
}
.img image{
	width:121rpx;
	height: 121rpx;
	border-radius: 50%;
}
.text {
	width: 100%;
	display: block;
	margin-left: 10rpx;
	text-align: left;
}
.num{
	width: 44rpx;
	height: 44rpx;
	background-color:#F36D11;
	border-radius: 50%;
	    margin-left: 20rpx;
}

